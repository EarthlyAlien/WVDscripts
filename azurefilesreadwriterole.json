{
  "Name": "AFReadWriteRole",
  "Id": null,
  "IsCustom": true,
  "Description": "Allows for read, write and delete access to Azure File Share over SMB",
  "Actions": [
   	"Microsoft.Storage/storageAccounts/fileServices/*"
  ],
